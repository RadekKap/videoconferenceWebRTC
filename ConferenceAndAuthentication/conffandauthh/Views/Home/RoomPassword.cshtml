
@{
    ViewBag.Title = "RoomPassword";
}

<h2>Pokój zabezpieczony hasłem</h2>
<form action="/?@ViewBag.roomName" method="post">

    <div role="form" class="form-inline">
        <div class="form-group">
            <p class="lead text-center">Podaj hasło: </p>
        </div>
        <div class="form-group">
            <input type="text" name="password">
        </div>
    </div>

    
    <input type="submit" class="btn btn-primary" value="Zatwierdź">
</form> 
